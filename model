def log_scan_to_supabase(image_name, confidence, severity, gradcam_url):
    url = os.getenv("https://dbmjyboohplgtcoqlmen.supabase.co")
    key = os.getenv("eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6ImRibWp5Ym9vaHBsZ3Rjb3FsbWVuIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NTY5NzI4MzUsImV4cCI6MjA3MjU0ODgzNX0.JhhxveJAEZ6oxiGl5gozMlgAkJFQ_-dWDyxwTfh6kQ0")

    headers = {
        "apikey": key,
        "Authorization": f"Bearer {eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6ImRibWp5Ym9vaHBsZ3Rjb3FsbWVuIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NTY5NzI4MzUsImV4cCI6MjA3MjU0ODgzNX0.JhhxveJAEZ6oxiGl5gozMlgAkJFQ_-dWDyxwTfh6kQ0}",
        "Content-Type": "
import { createClient } from '@supabase/supabase-js'

const supabaseUrl = 'https://dbmjyboohplgtcoqlmen.supabase.co'
const supabaseKey = process.env.eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6ImRibWp5Ym9vaHBsZ3Rjb3FsbWVuIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NTY5NzI4MzUsImV4cCI6MjA3MjU0ODgzNX0.JhhxveJAEZ6oxiGl5gozMlgAkJFQ_-dWDyxwTfh6kQ0
const supabase = createClient(https://dbmjyboohplgtcoqlmen.supabase.co, eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6ImRibWp5Ym9vaHBsZ3Rjb3FsbWVuIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NTY5NzI4MzUsImV4cCI6MjA3MjU0ODgzNX0.JhhxveJAEZ6oxiGl5gozMlgAkJFQ_-dWDyxwTfh6kQ0)"
    }

    payload = {
        "id": str(uuid.uuid4()),
        "timestamp": datetime.utcnow().isoformat(),
        "image_name": image_name,
        "confidence": confidence,
        "severity": severity,
        "gradcam_url": gradcam_url
    }

    requests.post(f"{url}/rest/v1/scan_logs", headers=headers, json=payload)
